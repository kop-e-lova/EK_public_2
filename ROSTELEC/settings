MAIN_URL = "https://b2c.passport.rt.ru" #URL страницы Ростелекома
phone_number = 'phone_number'  # вместо phone_number нужно ввести зарегистрированного пользователя (ОБЯЗАТЕЛЬНО К ЗАПОЛНЕНИЮ)
#при реальном тестировании использовались реальные персональные данные от учетной записи
#в данном коде персональные данные не приведены (оставлена пустая строка)
email = 'email'  #  вместо email необходимо ввести электронный адрес зарегистрированного пользователя (ОБЯЗАТЕЛЬНО К ЗАПОЛНЕНИЮ)
login = "Логин" # вместо Логин необходисо ввести логин зарегистрированного пользователя (ОБЯЗАТЕЛЬНО К ЗАПОЛНЕНИЮ)
account = 'account'  # вместо account необходимо ввести корректный лицевой счет зарегистрированного пользователя (ОБЯЗАТЕЛЬНО К ЗАПОЛНЕНИЮ)
password = 'password'  # вместо password необходимо ввести проль от УЗ зарегистрированного пользователя (ОБЯЗАТЕЛЬНО К ЗАПОЛНЕНИЮ)
